<?xml version="1.0" encoding="utf-8"?>
<layer-list
        xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:height="20dp" android:width="40dp">
        <shape android:shape="rectangle">
            <solid android:color="#E8E5FF"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>

    <item android:top="-3dp" android:bottom="-3dp" android:start="-3dp" android:end="17dp">
        <shape android:shape="oval">
            <solid android:color="#9437FF"/>
        </shape>
    </item>

</layer-list>