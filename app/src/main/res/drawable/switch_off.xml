<?xml version="1.0" encoding="utf-8"?>
<layer-list
        xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:height="15dp" android:width="30dp">
        <shape android:shape="rectangle">
            <solid android:color="#E8E5FF"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>

    <item android:top="-2dp" android:bottom="-2dp" android:start="-2dp" android:end="14dp">
        <shape android:shape="oval">
            <solid android:color="#9437FF"/>
        </shape>
    </item>

</layer-list>